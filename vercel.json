{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "cd react-flow-app && npm run build",
  "outputDirectory": "react-flow-app/dist",
  "installCommand": "cd react-flow-app && npm install",
  "rewrites": [{ "source": "/((?!assets/).*)", "destination": "/index.html" }]
}
